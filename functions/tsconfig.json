{
  "compilerOptions": {
    "module": "esnext",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "outDir": "lib",
    "sourceMap": true,
    "strict": true,
    "target": "es2017",
    "noEmit": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "@models/*": ["../models/*"]
    }
  },
  "include": ["src"]
}
